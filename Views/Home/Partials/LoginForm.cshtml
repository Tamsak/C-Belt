@model LoginUser

<h2>Log In</h2>
<form asp-action="Login" method="post">
    <div class="form-group">
        <span asp-validation-for="LogEmail"></span>
        <label asp-for="LogEmail"></label>
        <input class="form-control" asp-for="LogEmail">
    </div>
    <div class="form-group">
        <span asp-validation-for="Confirm"></span>
        <label asp-for="Confirm">Password</label>
        <input class="form-control" asp-for="Confirm">
    </div>
    <input type="submit" value="LogIn">
</form>